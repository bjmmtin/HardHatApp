{
  "name": "hardhat-project",
  "scripts": {
    "test": "yarn compile && npx hardhat test",
    "coverage": "npx hardhat coverage && cat coverage/lcov.info | coveralls",
    "coverage-rate": "npx hardhat coverage && cat coverage/lcov.info | coveralls",
    "compile": "npm run prettier && npx hardhat compile",
    "deploy": "hardhat deploy",
    "size": "yarn run hardhat size-contracts --no-compile",
    "prettier:solidity": "npx prettier --write contracts/.",
    "prettier:typescript": "npx prettier --write test/.",
    "prettier": "npx prettier --write contracts/. test/.",
    "lint": "eslint . --max-warnings 0 --ext .ts",
    "docs" : "npm run prettier && yarn hardhat docgen"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.4",
    "@nomiclabs/hardhat-solhint": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/hardhat-upgrades": "^1.22.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.8",
    "fast-check": "^3.0.0",
    "fs-extra": "^9.0.1",
    "hardhat": "^2.14.0",
    "hardhat-console": "^1.0.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.10",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.1",
    "merkletreejs": "^0.2.32",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "solidity-docgen": "^0.6.0-beta.35"
  }
}
